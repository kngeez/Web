function stop_call_support_login()
{
  var fieldPassPartners=document.getElementById("inputPassWizard_login");
  var fieldUserPartners=document.getElementById("inputSupportWizard_login");
  if(fieldPassPartners.value!="" && fieldUserPartners.value!="")
  {
	  document.getElementById("inputSupport_login").value=fieldUserPartners.value;
	  document.getElementById("inputPass_login").value=fieldPassPartners.value;

    var loginid=document.getElementById('inputSupportWizard_login').value;
    var who=loginid.substring(0,3);
    if(who=="NMP") var destination='partners';
    else var destination='support';
    var url = document.location.toString();
    var url2_array=url.split("/");
    //linda  
    var url_base=url2_array[0]+'/'+url2_array[1]+'/'+url2_array[2]+'/'+url2_array[3];
    
    var datastr ='id='+fieldUserPartners.value+'&pwd='+fieldPassPartners.value+'&what=&path='+nomachine_base_path;
    var url=nomachine_base_path+'verify_account.php';
    //alert('url='+url);
      
  $.ajax({
  type: "POST",
  url: url,
  data: datastr,
  cache: false,
  success: function(msg)
  {
    msg1=msg.substr(0,5);
    if(msg1=='valid')
    {
      document.getElementById('user-login-form_login').action=url_base+'/?destination='+destination;
	    document.getElementById('user-login-form_login').submit();
    }
    else
    {
      document.getElementById("errorloginok").style.display='none';
      document.getElementById("errorlogin").style.display='block';
      $("div#errorlogin").html("Invalid Id and/or Password");
    }
  },
  error: function(){
    alert("Error!");
  }
  });
    /*document.getElementById('user-login-form_login').action=url_base+'/?destination='+destination;
	  document.getElementById('user-login-form_login').submit();*/
  }
}


